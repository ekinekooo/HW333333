<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.homework333333.FirstFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first" >
        <action
            android:id="@+id/action_firstFragment_to_filmFragment"
            app:destination="@id/filmFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/secondFragment"
        android:name="com.example.homework333333.SecondFragment"
        android:label="fragment_second"
        tools:layout="@layout/fragment_second" />
    <fragment
        android:id="@+id/fourthFragment"
        android:name="com.example.homework333333.FourthFragment"
        android:label="fragment_fourth"
        tools:layout="@layout/fragment_fourth" />
    <fragment
        android:id="@+id/thirdFragment"
        android:name="com.example.homework333333.ThirdFragment"
        android:label="fragment_third"
        tools:layout="@layout/fragment_third" />
    <fragment
        android:id="@+id/activeFragment"
        android:name="com.example.homework333333.ActiveFragment"
        android:label="fragment_active"
        tools:layout="@layout/fragment_active" >
        <action
            android:id="@+id/action_activeFragment_to_shareFragment"
            app:destination="@id/shareFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/shareFragment"
        android:name="com.example.homework333333.MessageFragment"
        android:label="fragment_share"
        tools:layout="@layout/fragment_message" />
    <fragment
        android:id="@+id/filmFragment"
        android:name="com.example.homework333333.FilmFragment"
        android:label="FilmFragment" />
</navigation>